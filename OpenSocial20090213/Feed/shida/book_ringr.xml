<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="BookRingr">
    <Require feature="opensocial-0.8" />
    <Optional feature="content-rewrite">
      <Param name="include-urls"></Param>
      <Param name="exclude-urls">.*</Param>
      <Param name="include-tags"></Param>
    </Optional>
  </ModulePrefs>
  <UserPref name="booklog_user_id" display_name="ブクログのユーザーID" datatype="string" />
  <Content type="html">
    <![CDATA[
      <script type="text/javascript" src="http://hackathon-jp.googlecode.com/svn/trunk/OpenSocial20090213/Feed/shida/javascripts/jquery-1.3.js"></script>
      <script type="text/javascript">
var params = {};
params[gadgets.io.RequestParameters.METHOD] = gadgets.io.MethodType.GET;
params[gadgets.io.RequestParameters.CONTENT_TYPE] = gadgets.io.ContentType.FEED;

gadgets.io.makeRequest("http://booklog.jp/users/yshida/feed/RSS1", function(obj) {
 var booklog = obj.data;
 output(obj.data.Entry);
 console.log(obj.data.Entry);
}, params);
      </script>
      <div id="profile"></div>
    ]]>
  </Content>
</Module>
